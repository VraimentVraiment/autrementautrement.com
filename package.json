  "author": "Lucas Poulain (ShallowRed)",
  "license": "MIT",
  "homepage": "https://github.com/VraimentVraiment/autrementautrement.com#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VraimentVraiment/autrementautrement.com.git"
  },
  "bugs": {
