    "@iconify-json/ri": "^1.2.0",
    "@unocss/cli": "^0.62.3",
    "@unocss/reset": "^0.62.3",
    "@unocss/transformer-directives": "^0.62.3",
    "concurrently": "^9.0.0",
    "eslint": "^9.10.0",
    "glob": "^11.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-normalize": "^13.0.0",
    "sass": "^1.78.0",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier-scss": "^1.0.0",
